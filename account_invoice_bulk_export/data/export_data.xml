<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <!-- Secuencia para nombres de exportación (futuro uso) -->
        <record id="seq_invoice_export" model="ir.sequence">
            <field name="name">Exportación de Facturas</field>
            <field name="code">invoice.export</field>
            <field name="prefix">EXP-%(year)s/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>
</odoo>

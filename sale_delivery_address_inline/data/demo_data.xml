<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Clientes de demostración -->
        <record id="partner_demo_distributor" model="res.partner">
            <field name="name">Distribuciones Demo, S.L.</field>
            <field name="is_distributor" eval="True"/>
            <field name="street">Avenida Principal 123</field>
            <field name="city">Madrid</field>
            <field name="zip">28001</field>
            <field name="country_id" ref="base.es"/>
            <field name="email">info@distribuciones-demo.com</field>
            <field name="phone">+34 911 222 333</field>
            <field name="website">www.distribuciones-demo.com</field>
            <field name="company_id" ref="base.main_company"/>
        </record>

        <!-- Direcciones de entrega para el distribuidor -->
        <record id="partner_demo_distributor_address1" model="res.partner">
            <field name="name">Almacén Central</field>
            <field name="type">delivery</field>
            <field name="parent_id" ref="partner_demo_distributor"/>
            <field name="street">Polígono Industrial Norte, Nave 7</field>
            <field name="city">Madrid</field>
            <field name="zip">28914</field>
            <field name="country_id" ref="base.es"/>
            <field name="email">almacen@distribuciones-demo.com</field>
            <field name="phone">+34 911 444 555</field>
            <field name="company_id" ref="base.main_company"/>
        </record>

        <record id="partner_demo_distributor_address2" model="res.partner">
            <field name="name">Centro Logístico Barcelona</field>
            <field name="type">delivery</field>
            <field name="parent_id" ref="partner_demo_distributor"/>
            <field name="street">Calle Industria 45</field>
            <field name="city">Barcelona</field>
            <field name="zip">08001</field>
            <field name="country_id" ref="base.es"/>
            <field name="email">barcelona@distribuciones-demo.com</field>
            <field name="phone">+34 933 222 111</field>
            <field name="company_id" ref="base.main_company"/>
        </record>

        <record id="partner_demo_distributor_address3" model="res.partner">
            <field name="name">Delegación Sur</field>
            <field name="type">delivery</field>
            <field name="parent_id" ref="partner_demo_distributor"/>
            <field name="street">Avenida de Andalucía 78</field>
            <field name="city">Sevilla</field>
            <field name="zip">41001</field>
            <field name="country_id" ref="base.es"/>
            <field name="email">sevilla@distribuciones-demo.com</field>
            <field name="phone">+34 955 666 777</field>
            <field name="company_id" ref="base.main_company"/>
        </record>

        <!-- Cliente normal (no distribuidor) -->
        <record id="partner_demo_normal" model="res.partner">
            <field name="name">Comercio Local Demo</field>
            <field name="is_distributor" eval="False"/>
            <field name="street">Calle Comercio 56</field>
            <field name="city">Valencia</field>
            <field name="zip">46001</field>
            <field name="country_id" ref="base.es"/>
            <field name="email">info@comerciolocal-demo.com</field>
            <field name="phone">+34 963 111 222</field>
            <field name="company_id" ref="base.main_company"/>
        </record>
    </data>
</odoo>
